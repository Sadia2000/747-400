<PropertyList>
    <path>747-400.ac</path>

    <!-- Submodels -->
    <model>
        <name>engines-ge</name>
        <path>Aircraft/747-400/Models/GE-CF6.xml</path>
    </model>
    <model>
        <name>engines-rr</name>
        <path>Aircraft/747-400/Models/RR-RB211.ac</path>
    </model>
    <animation>
        <type>select</type>
        <object-name>engines-rr</object-name>
        <condition>
            <equals>
                <property>sim/aircraft-id</property>
                <value>747-400-RR</value>
            </equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>engines-ge</object-name>
        <condition>
            <equals>
                <property>sim/aircraft-id</property>
                <value>747-400-GE</value>
            </equals>
        </condition>
    </animation>

    <model>
        <name>Pushback</name>
        <path>Aircraft/747-400/Models/Autopush/Goldhofert.xml</path>
        <offsets>
            <x-m>-21.993</x-m>
            <y-m>  0.000</y-m>
            <z-m> -5.625</z-m>
        </offsets>
    </model>
    <animation>
        <object-name>Pushback</object-name>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>5000</max-m>
    </animation>
    <model>
        <name>cockpit</name>
        <path>Aircraft/747-400/Models/Cockpit/cockpit.xml</path>
    </model>
    <animation>
        <object-name>cockpit</object-name>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>500</max-m>
    </animation>

    <!-- Fuselage Effects -->
    <effect>
        <inherits-from>Effects/747-400_chrome</inherits-from>
        <object-name>v-stab.chrome</object-name>
        <object-name>winglet.chromeL</object-name>
        <object-name>winglet.chromeR</object-name>
        <object-name>wing.edgeL</object-name>
        <object-name>wing.edgeR</object-name>
    </effect>

