   <animation>
    	<type>pick</type>
    	<object-name>btn.at.arm</object-name>
    	<visible>1</visible>
    	<action>
    		<name>AT Arm</name>
    		<button>0</button>
    		<repeatable>true</repeatable> 
    		<binding>
				<command>nasal</command>
				<script>
				if (getprop("/it-autoflight/custom/at-arm") == 0) {
					setprop("/it-autoflight/custom/at-arm", 1);
				} else if (getprop("/it-autoflight/custom/at-arm") == 1) {
					setprop("/it-autoflight/custom/at-arm", 0);
					setprop("/it-autoflight/input/athr", 0);
				}
				</script>
    		</binding>
    	</action>
   </animation>
