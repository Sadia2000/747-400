        <!-- Flaps -->

        <flap>
            <name>flaps</name>
            <mode>in-transit</mode>
            <path>Aircraft/747-400/Sounds/FGOS_FlapXtndWClick.wav</path>
            <property>/surface-positions/flap-pos-norm</property>
            <volume>
                <factor>0.15</factor>
            </volume>
        </flap>

        <stall>
            <name>stall</name>
            <path>Sounds/stall.wav</path>
            <condition>
                <property>/sim/alarms/stall-warning</property>
            </condition>
            <volume>
                <property>/velocities/airspeed-kt</property>
                <min>30.0</min>
            </volume>
        </stall>

        <shaker>
            <name>shaker</name>
            <mode>looped</mode>
            <path>Sounds/shaker.wav</path>
            <condition>
                <and>
                    <property>/fdm/jsbsim/systems/stick-shaker-norm</property>
                    <greater-than>
                        <property>/velocities/airspeed-kt</property>
                        <value>10</value> <!-- avoid spurious ground activation -->
                    </greater-than>
                </and>
            </condition>
            <volume>
                <property>/velocities/airspeed-kt</property>
                <min>30.0</min>
            </volume>
        </shaker>

        <!-- Warnings -->

        <config-TO>
            <name>config-flaps</name>
            <mode>looped</mode>
            <path>Aircraft/747-400/Sounds/FGOS_Configuration_Warn.wav</path>
            <condition>
                <and>
                    <or>
                        <and>
                            <equals>
                                <property>/gear/gear[0]/wow</property>
                                <value>1</value>
                            </equals>
                            <less-than>
                                <property>/surface-positions/flap-pos-norm</property>
                                <value>0.33</value>
                            </less-than>
                            <greater-than>
                                <property>/controls/engines/engine[1]/throttle</property>
                                <value>0.5</value>
                            </greater-than>
                            <greater-than>
                                <property>/controls/engines/engine[2]/throttle</property>
                                <value>0.5</value>
                            </greater-than>
                        </and>
                        <and>
                            <equals>
                                <property>/controls/gear/brake-parking</property>
                                <value>1</value>
                            </equals>
                            <greater-than>
                                <property>/controls/engines/engine[1]/throttle</property>
                                <value>0.5</value>
                            </greater-than>
                            <greater-than>
                                <property>/controls/engines/engine[2]/throttle</property>
                                <value>0.5</value>
                            </greater-than>
                        </and>
                    </or>
                    <or>
                        <equals>
                            <property>engines/engine[0]/running</property>
                            <value>true</value>
                        </equals>
                        <equals>
                            <property>engines/engine[1]/running</property>
                            <value>true</value>
                        </equals>
                        <equals>
                            <property>engines/engine[2]/running</property>
                            <value>true</value>
                        </equals>
                        <equals>
                            <property>engines/engine[3]/running</property>
                            <value>true</value>
                        </equals>
                    </or>
                </and>
            </condition>
        </config-TO>

        <overspeed>
            <name>overspeed</name>
            <mode>looped</mode>
            <path>Sounds/gear-hrn.wav</path>
            <condition>
                <greater-than>
                    <property>/velocities/mach</property>
                    <value>0.9</value>
                </greater-than>
            </condition>
        </overspeed>

        <fire>
            <name>fire</name>
            <mode>looped</mode>
            <path>Aircraft/747-400/Sounds/fire-warn.wav</path>
            <condition>
                <or>
                    <and>
                        <greater-than>
                            <property>/engines/engine[0]/on-fire</property>
                            <value>0</value>
                        </greater-than>
                        <not>
                            <property>/engines/engine[0]/extinguished</property>
                        </not>
                    </and>
                    <and>
                        <greater-than>
                            <property>/engines/engine[1]/on-fire</property>
                            <value>0</value>
                        </greater-than>
                        <not>
                            <property>/engines/engine[1]/extinguished</property>
                        </not>
                    </and>
                    <and>
                        <greater-than>
                            <property>/engines/engine[2]/on-fire</property>
                            <value>0</value>
                        </greater-than>
                        <not>
                            <property>/engines/engine[2]/extinguished</property>
                        </not>
                    </and>
                    <and>
                        <greater-than>
                            <property>/engines/engine[3]/on-fire</property>
                            <value>0</value>
                        </greater-than>
                        <not>
                            <property>/engines/engine[3]/extinguished</property>
                        </not>
                    </and>
                </or>
            </condition>
        </fire>

        <!-- Additions -->
        <switch>
            <name>seatbelt-sign</name>
            <mode>once</mode>
            <path>Aircraft/747-400/Sounds/FastenSeatbelt.wav</path>
            <condition>
                <property>controls/switches/seatbelt-sign</property>
            </condition>
            <volume>
                <factor>1</factor>
            </volume>
        </switch>

        <switch>
            <name>switch</name>
            <mode>once</mode>
            <path>Sounds/click.wav</path>
            <condition>
                <property>sim/sound/click</property>
            </condition>
            <volume>
                <factor>1</factor>
            </volume>
        </switch>

    </fx>

