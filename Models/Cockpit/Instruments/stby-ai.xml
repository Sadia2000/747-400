<?xml version="1.0"?>

<PropertyList>

<path>stby-ai.ac</path>

<animation>
 <type>rotate</type>
 <object-name>ball</object-name>
 <property>/orientation/roll-deg</property>
 <center>
  <x-m>-0.042</x-m>
  <y-m> 0</y-m>
  <z-m>-0.004</z-m>
 </center>
 <axis>
  <x>1</x>
  <y>0</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>ball</object-name>
 <property>/orientation/pitch-deg</property>
 <center>
  <x-m>-0.042</x-m>
  <y-m> 0</y-m>
  <z-m>-0.004</z-m>
 </center>
 <axis>
  <x>0</x>
  <y>1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>rollarrow</object-name>
 <property>/orientation/roll-deg</property>
 <axis>
  <x>1</x>
  <y>0</y>
  <z>0</z>
 </axis>
</animation>
<!--Custom Lite Light-->
<animation>
 <type>material</type> 
 <object-name>wings</object-name>
 <object-name>rollindex</object-name>
 <object-name>ball</object-name>
 <condition>
  <greater-than>		
   <property>/controls/lighting/panel-norm</property>
   <value>0</value>
  </greater-than>
 </condition>
 <emission>	
  <red>0.75</red>	
  <green>0.75</green>	
  <blue>0.75</blue>
 </emission>
</animation>
</PropertyList>
