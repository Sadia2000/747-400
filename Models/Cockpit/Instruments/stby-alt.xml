<?xml version="1.0"?>

<PropertyList>

<path>stby-alt.ac</path>
<!--Custom Lite Light-->
<animation>
 <type>material</type> 
 <object-name>Face</object-name>
 <object-name>alt4</object-name>
 <object-name>alt3</object-name>
 <object-name>alt2</object-name>
 <object-name>alt1</object-name>
 <condition>
  <greater-than>		
   <property>/controls/lighting/panel-overhead-norm</property>
   <value>0</value>
  </greater-than>
 </condition>
 <emission>	
  <red>0.75</red>	
  <green>0.75</green>	
  <blue>0.75</blue>
 </emission>
</animation>

<animation>
 <type>rotate</type>
 <object-name>Needle1</object-name>
 <property>/instrumentation/altimeter/indicated-altitude-ft</property>
 <factor>0.36</factor>
 <axis>
  <x>-1</x>
  
  
 </axis>
</animation>

<animation>
 <type>textranslate</type>
 <object-name>alt1</object-name>
 <property>/instrumentation/altimeter/indicated-altitude-ft</property>
 <factor>0.01</factor>
 <axis>
  
  <y>1</y>
  
 </axis>
</animation>

<animation>
 <type>textranslate</type>
 <object-name>alt2</object-name>
 <property>/instrumentation/altimeter/indicated-altitude-ft</property>
 <factor>0.001</factor>
 <step>100</step>
 <scroll>20</scroll>
 <axis>
  
  <y>1</y>
  
 </axis>
</animation>

<animation>
 <type>textranslate</type>
 <object-name>alt3</object-name>
 <property>/instrumentation/altimeter/indicated-altitude-ft</property>
 <factor>0.0001</factor>
 <step>1000</step>
 <scroll>20</scroll>
 <axis>
  
  <y>1</y>
  
 </axis>
</animation>

<animation>
 <type>textranslate</type>
 <object-name>alt4</object-name>
 <property>/instrumentation/altimeter/indicated-altitude-ft</property>
 <factor>0.00001</factor>
 <step>10000</step>
 <scroll>20</scroll>
 <axis>
  
  <y>1</y>
  
 </axis>
</animation>

<animation>
 <type>textranslate</type>
 <object-name>press1</object-name>
 <property>/instrumentation/altimeter/setting-inhg</property>
 <factor>10.0</factor>
 <step>0.01</step>
 <axis>
  
  <y>1</y>
  
 </axis>
</animation>

<animation>
 <type>textranslate</type>
 <object-name>press2</object-name>
 <property>/instrumentation/altimeter/setting-inhg</property>
 <factor>1.0</factor>
 <step>0.1</step>
 <axis>
  
  <y>1</y>
  
 </axis>
</animation>

<animation>
 <type>textranslate</type>
 <object-name>press3</object-name>
 <property>/instrumentation/altimeter/setting-inhg</property>
 <factor>0.1</factor>
 <step>1</step>
 <axis>
  
  <y>1</y>
  
 </axis>
</animation>

<animation>
 <type>textranslate</type>
 <object-name>press4</object-name>
 <property>/instrumentation/altimeter/setting-inhg</property>
 <factor>0.01</factor>
 <step>10</step>
 <axis>
  
  <y>1</y>
  
 </axis>
</animation>

</PropertyList>
