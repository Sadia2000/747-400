<?xml version="1.0"?>

<PropertyList>

<path>stby-asi.ac</path>

<!--Custom Lite Light-->
<animation>
 <type>material</type> 
 <object-name>Face</object-name>
 <object-name>Needle1</object-name>
 <condition>
  <greater-than>		
   <property>/controls/lighting/panel-norm</property>
   <value>0</value>
  </greater-than>
 </condition>
 <emission>	
  <red>0.75</red>	
  <green>0.75</green>	
  <blue>0.75</blue>
 </emission>
</animation>
<animation>
 <type>rotate</type>
 <object-name>Needle1</object-name>
 <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
 <interpolation>
  <entry>
   <ind>0</ind>
   <dep>-9.5</dep>
  </entry>
  <entry>
   <ind>60</ind>
   <dep>0</dep>
  </entry>
  <entry>
   <ind>250</ind>
   <dep>244</dep>
  </entry>
  <entry>
   <ind>300</ind>
   <dep>265.5</dep>
  </entry>
  <entry>
   <ind>450</ind>
   <dep>331</dep>
  </entry>
 </interpolation>
 <axis>
  <x>-1</x>
  <y>0</y>
  <z>0</z>
 </axis>
</animation>

</PropertyList>


